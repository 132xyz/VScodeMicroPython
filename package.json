{
  "name": "mpy",
  "icon": "media/logo.png",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "0.1.43",
  "publisher": "WebForks",
  "engines": {
    "vscode": "^1.84.0"
  },
  "extensionDependencies": [],
  "extensionPack": [],
  "extensionKind": [
    "workspace"
  ],
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "categories": [
    "Programming Languages",
    "Debuggers"
  ],
  "activationEvents": [
    "onLanguage:python",
    "onStartupFinished"
  ],
  "recommends": [
    "ms-python.vscode-pylance"
  ],
  "main": "./dist/core/extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "@vscode/vsce package",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.30",
    "@types/vscode": "^1.84.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.4.0",
    "@vscode/vsce": "^2.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/132xyz/VScodeMicroPython"
  },
  "contributes": {
    "viewsWelcome": [
      {
        "view": "microPythonWorkBenchFsView",
        "contents": "%viewsWelcome.noPort.contents%",
        "when": "!microPythonWorkBench.hasPort"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "microPythonWorkBenchFs",
          "title": "%viewsContainers.activitybar.title%",
          "icon": "./media/activitybar.svg"
        }
      ]
    },
    "customEditors": [],
    "css": [
      "./media/esp32fs.css"
    ],
    "views": {
      "microPythonWorkBenchFs": [
        {
          "id": "microPythonWorkBenchFsView",
          "name": "%views.microPythonWorkBenchFs.files.name%",
          "icon": "$(files)"
        },
        {
          "id": "microPythonWorkBenchActionsView",
          "name": "%views.microPythonWorkBenchFs.boardActions.name%",
          "icon": "$(tools)"
        },
        {
          "id": "microPythonWorkBenchSyncView",
          "name": "%views.microPythonWorkBenchFs.syncFiles.name%",
          "icon": "$(sync)"
        }
      ]
    },
    "commands": [
      {
        "command": "microPythonWorkBench.refresh",
        "title": "%commands.refresh.title%",
        "icon": "$(refresh)"
      },
      {
        "command": "microPythonWorkBench.openFile",
        "title": "%commands.openFile.title%"
      },
      {
        "command": "microPythonWorkBench.uploadActiveFile",
        "title": "%commands.uploadActiveFile.title%"
      },
      {
        "command": "microPythonWorkBench.newFileBoardAndLocal",
        "title": "%commands.newFileBoardAndLocal.title%",
        "icon": "$(new-file)"
      },
      {
        "command": "microPythonWorkBench.newFileInTree",
        "title": "%commands.newFileInTree.title%",
        "icon": "$(new-file)"
      },
      {
        "command": "microPythonWorkBench.newFolderInTree",
        "title": "%commands.newFolderInTree.title%",
        "icon": "$(new-folder)"
      },
      {
        "command": "microPythonWorkBench.renameNode",
        "title": "%commands.renameNode.title%",
        "icon": "$(edit)"
      },
      {
        "command": "microPythonWorkBench.pickPort",
        "title": "%commands.pickPort.title%",
        "icon": "$(plug)"
      },
      {
        "command": "microPythonWorkBench.refreshPorts",
        "title": "%commands.refreshPorts.title%",
        "icon": "$(refresh)"
      },
      {
        "command": "microPythonWorkBench.setPort",
        "title": "%commands.setPort.title%"
      },
      {
        "command": "microPythonWorkBench.mkdir",
        "title": "%commands.mkdir.title%"
      },
      {
        "command": "microPythonWorkBench.openSerial",
        "title": "%commands.openSerial.title%",
        "icon": "$(terminal)"
      },
      {
        "command": "microPythonWorkBench.openRepl",
        "title": "%commands.openRepl.title%",
        "icon": "$(terminal)"
      },
      {
        "command": "microPythonWorkBench.runActiveFile",
        "title": "%commands.runActiveFile.title%",
        "icon": "$(play)"
      },
      {
        "command": "microPythonWorkBench.stop",
        "title": "%commands.stop.title%",
        "icon": "$(debug-stop)"
      },
      {
        "command": "microPythonWorkBench.softReset",
        "title": "%commands.softReset.title%",
        "icon": "$(debug-restart)"
      },
      {
        "command": "microPythonWorkBench.syncBaseline",
        "title": "%commands.syncBaseline.title%",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "microPythonWorkBench.checkDiffs",
        "title": "%commands.checkDiffs.title%",
        "icon": "$(diff)"
      },
      {
        "command": "microPythonWorkBench.syncDiffsLocalToBoard",
        "title": "%commands.syncDiffsLocalToBoard.title%",
        "icon": "$(export)"
      },
      {
        "command": "microPythonWorkBench.syncDiffsBoardToLocal",
        "title": "%commands.syncDiffsBoardToLocal.title%",
        "icon": "$(import)"
      },
      {
        "command": "microPythonWorkBench.syncBaselineFromBoard",
        "title": "%commands.syncBaselineFromBoard.title%",
        "icon": "$(cloud-download)"
      },
      {
        "command": "microPythonWorkBench.serialSendCtrlC",
        "title": "%commands.serialSendCtrlC.title%",
        "icon": "$(debug-pause)"
      },
      {
        "command": "microPythonWorkBench.openFileFromLocal",
        "title": "%commands.openFileFromLocal.title%",
        "icon": "$(file-directory)"
      },
      {
        "command": "microPythonWorkBench.flashMicroPython",
        "title": "%commands.flashMicroPython.title%"
      },
      {
        "command": "microPythonWorkBench.syncFileLocalToBoard",
        "title": "%commands.syncFileLocalToBoard.title%",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "microPythonWorkBench.syncFileBoardToLocal",
        "title": "%commands.syncFileBoardToLocal.title%",
        "icon": "$(cloud-download)"
      },
      {
        "command": "microPythonWorkBench.delete",
        "title": "%commands.delete.title%"
      },
      {
        "command": "microPythonWorkBench.deleteBoardAndLocal",
        "title": "%commands.deleteBoardAndLocal.title%",
        "icon": {
          "light": "./media/trash-red.svg",
          "dark": "./media/trash-red.svg"
        }
      },
      {
        "command": "microPythonWorkBench.runFromView",
        "title": "%commands.runFromView.title%"
      },
      {
        "command": "microPythonWorkBench.syncBaselineFromView",
        "title": "%commands.syncBaselineFromView.title%",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "microPythonWorkBench.syncBaselineFromBoardFromView",
        "title": "%commands.syncBaselineFromBoardFromView.title%",
        "icon": "$(cloud-download)"
      },
      {
        "command": "microPythonWorkBench.checkDiffsFromView",
        "title": "%commands.checkDiffsFromView.title%",
        "icon": "$(diff)"
      },
      {
        "command": "microPythonWorkBench.syncDiffsLocalToBoardFromView",
        "title": "%commands.syncDiffsLocalToBoardFromView.title%",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "microPythonWorkBench.syncDiffsBoardToLocalFromView",
        "title": "%commands.syncDiffsBoardToLocalFromView.title%",
        "icon": "$(cloud-download)"
      },
      {
        "command": "microPythonWorkBench.runActiveFileFromView",
        "title": "%commands.runActiveFileFromView.title%",
        "icon": "$(play)"
      },
      {
        "command": "microPythonWorkBench.openReplFromView",
        "title": "%commands.openReplFromView.title%",
        "icon": "$(terminal)"
      },
      {
        "command": "microPythonWorkBench.deleteAllBoard",
        "title": "%commands.deleteAllBoard.title%",
        "icon": {
          "light": "./media/trash-red.svg",
          "dark": "./media/trash-red.svg"
        }
      },
      {
        "command": "microPythonWorkBench.deleteAllBoardFromView",
        "title": "%commands.deleteAllBoardFromView.title%",
        "icon": {
          "light": "./media/trash-red.svg",
          "dark": "./media/trash-red.svg"
        }
      },
      {
        "command": "microPythonWorkBench.toggleWorkspaceAutoSync",
        "title": "%commands.toggleWorkspaceAutoSync.title%"
      },
      {
        "command": "microPythonWorkBench.installMpremote",
        "title": "%commands.installMpremote.title%",
        "icon": "$(package)"
      },
      {
        "command": "microPythonWorkBench.toggleCodeCompletion",
        "title": "%commands.toggleCodeCompletion.title%"
      },
      {
        "command": "microPythonWorkBench.installMpremoteAutomatically",
        "title": "%commands.installMpremoteAutomatically.title%",
        "icon": "$(package)"
      },
      {
        "command": "microPythonWorkBench.checkMpremoteStatus",
        "title": "%commands.checkMpremoteStatus.title%"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "microPythonWorkBench.syncBaselineFromView",
          "when": "view == microPythonWorkBenchSyncView",
          "group": "navigation@0"
        },
        {
          "command": "microPythonWorkBench.syncBaselineFromBoardFromView",
          "when": "view == microPythonWorkBenchSyncView",
          "group": "navigation@1"
        },
        {
          "command": "microPythonWorkBench.checkDiffsFromView",
          "when": "view == microPythonWorkBenchSyncView",
          "group": "navigation@3"
        },
        {
          "command": "microPythonWorkBench.syncDiffsLocalToBoardFromView",
          "when": "view == microPythonWorkBenchSyncView",
          "group": "navigation@4"
        },
        {
          "command": "microPythonWorkBench.syncDiffsBoardToLocalFromView",
          "when": "view == microPythonWorkBenchSyncView",
          "group": "navigation@5"
        },
        {
          "command": "microPythonWorkBench.deleteAllBoardFromView",
          "when": "view == microPythonWorkBenchSyncView",
          "group": "navigation@9"
        },
        {
          "command": "microPythonWorkBench.runActiveFileFromView",
          "when": "view == microPythonWorkBenchActionsView",
          "group": "navigation@0"
        },
        {
          "command": "microPythonWorkBench.openReplFromView",
          "when": "view == microPythonWorkBenchActionsView",
          "group": "navigation@1"
        },
        {
          "command": "microPythonWorkBench.serialSendCtrlC",
          "when": "view == microPythonWorkBenchActionsView",
          "group": "navigation@2"
        },
        {
          "command": "microPythonWorkBench.refresh",
          "when": "view == microPythonWorkBenchFsView",
          "group": "navigation@0"
        },
        {
          "command": "microPythonWorkBench.pickPort",
          "when": "view == microPythonWorkBenchFsView",
          "group": "navigation@1"
        },
        {
          "command": "microPythonWorkBench.checkDiffsFromView",
          "when": "view == microPythonWorkBenchSyncView",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "microPythonWorkBench.renameNode",
          "when": "view == microPythonWorkBenchFsView && (viewItem == file || viewItem == dir)",
          "group": "2_edit@1"
        },
        {
          "command": "microPythonWorkBench.openFile",
          "when": "view == microPythonWorkBenchFsView && viewItem == file",
          "group": "1_open@1"
        },
        {
          "command": "microPythonWorkBench.openFileFromLocal",
          "when": "view == microPythonWorkBenchFsView && viewItem == file",
          "group": "1_open@2"
        },
        {
          "command": "microPythonWorkBench.syncFileLocalToBoard",
          "when": "view == microPythonWorkBenchFsView && viewItem == file",
          "group": "2_sync@1"
        },
        {
          "command": "microPythonWorkBench.syncFileBoardToLocal",
          "when": "view == microPythonWorkBenchFsView && viewItem == file",
          "group": "2_sync@2"
        },
        {
          "command": "microPythonWorkBench.newFileInTree",
          "when": "view == microPythonWorkBenchFsView && (viewItem == dir || viewItem == file)",
          "group": "0_create@0"
        },
        {
          "command": "microPythonWorkBench.newFolderInTree",
          "when": "view == microPythonWorkBenchFsView && (viewItem == dir || viewItem == file)",
          "group": "0_create@1"
        },
        {
          "command": "microPythonWorkBench.delete",
          "when": "view == microPythonWorkBenchFsView && (viewItem == file || viewItem == dir)",
          "group": "3_delete@1"
        },
        {
          "command": "microPythonWorkBench.deleteBoardAndLocal",
          "when": "view == microPythonWorkBenchFsView && (viewItem == file || viewItem == dir)",
          "group": "3_delete@2"
        },
        {
          "command": "microPythonWorkBench.newFileBoardAndLocal",
          "when": "view == microPythonWorkBenchFsView && !viewItem",
          "group": "0_create@0"
        },
        {
          "command": "microPythonWorkBench.newFileInTree",
          "when": "view == microPythonWorkBenchFsView && !viewItem",
          "group": "0_create@1"
        },
        {
          "command": "microPythonWorkBench.newFolderInTree",
          "when": "view == microPythonWorkBenchFsView && !viewItem",
          "group": "0_create@2"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "%configuration.title%",
      "properties": {
        "microPythonWorkBench.autoSyncOnSave": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.autoSyncOnSave.description%"
        },
        "microPythonWorkBench.connect": {
          "type": "string",
          "default": "auto",
          "description": "%configuration.connect.description%"
        },
        "microPythonWorkBench.serialAutoSuspend": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.serialAutoSuspend.description%"
        },
        "microPythonWorkBench.replRestoreBehavior": {
          "type": "string",
          "enum": [
            "runChanged",
            "executeBootMain",
            "openReplEmpty",
            "none"
          ],
          "default": "none",
          "description": "%configuration.replRestoreBehavior.description%"
        },
        "microPythonWorkBench.interruptOnConnect": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.interruptOnConnect.description%"
        },
        "microPythonWorkBench.strictConnect": {
          "type": "boolean",
          "default": true,
          "description": "%configuration.strictConnect.description%"
        },
        "microPythonWorkBench.preListDelayMs": {
          "type": "number",
          "default": 150,
          "description": "%configuration.preListDelayMs.description%"
        },
        "microPythonWorkBench.rootPath": {
          "type": "string",
          "default": "/",
          "description": "%configuration.rootPath.description%"
        },
        "microPythonWorkBench.syncLocalRoot": {
          "type": "string",
          "default": "",
          "description": "%configuration.syncLocalRoot.description%"
        },
        "microPythonWorkBench.usePyRawList": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.usePyRawList.description%"
        },
        "microPythonWorkBench.baudRate": {
          "type": "number",
          "default": 115200,
          "description": "%configuration.baudRate.description%"
        },
        "microPythonWorkBench.pythonPath": {
          "type": "string",
          "default": "",
          "description": "%configuration.pythonPath.description%"
        },
        "microPythonWorkBench.enableCodeCompletion": {
          "type": "boolean",
          "default": false,
          "description": "%configuration.enableCodeCompletion.description%"
        }
      }
    }
  }
}
